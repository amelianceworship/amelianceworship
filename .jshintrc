{
    "camelcase" : true,         // Названия переменных в camelCase стиле
    "undef" : true,             // Все не глобальные переменные должны быть объявлены до их использования.
    "quotmark" : true,          // Согласованность стиля кавычек
    "maxlen" : 120,             // Максимальная длина строки
    "strict" : true,            // Обязательно строгое использование.
    "trailing" : true,          // Запрещены пробелы на концах.
    "curly" : false,            // Фигурные кавычки обязательны для каждого нового блока или области.
    "browser" : true,           // Стандартный браузер `window`, `document`.
    "devel" : true,             // Разрешена панель разработчика `console.log();`.
    "jquery" : true,            // Опция определяет глобальные переменные, предоставляемые библиотекой jQuery.
    "esversion" : 9,            // Включить функции, представленные в ECMAScript 8.
    "node" : true,              // Определяет доступные глобальные переменные.
    "asi" : false,              // Следит за точкой с запятой
    "trailingcomma" : false,    // Запятая в конце элементов масива (и может еще где?) (default: false)
    "eqeqeq" : true,            // Следит за строгим сравнением === и !== вместо == и != (default: false)
    "unused" : true,            // Следит за объявлеными, но не используемыми переменными (default: false)
    "forin" : true,             // Следит за тем поставлена ли конструкция, которая проверяет являеться ли объектом передаваемое в конструкцию for (key in obj) { if (obj.hasOwnProperty(key)) {} } {
    "leanswitch" : true,        // Проверяет switch на ненужные предложения
    "maxdepth" : 3              // Максимальное количество вложенностей
    }
    // "latedef" : "nofunc"             // не допускает использование переменных до их явного объявления
    // "maxparams" : 4                  // Максимальное количество параметров передаваемых в функцию
